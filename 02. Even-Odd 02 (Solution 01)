#include <cstring>
#include <iostream>

using namespace std;

int main()
{
    int i, n;
    cin >> n;

    for(i = 0; i < n; i++)
    {
        char a[100];
        cin >> a;

        if(a[strlen(a) - 1]%2 == 0)
            cout << "even" << endl;
        else
            cout << "odd" << endl;
    }
    
    return 0;
}
