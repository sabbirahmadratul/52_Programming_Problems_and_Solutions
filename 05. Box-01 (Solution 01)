#include <iostream>

using namespace std;

int main()
{
    int i, j, n, h;
    cin >> n;
	
    while(n--)
    {
        cin >> h;
		
        for(i = 0; i < h; i++)
        {
           for(j = 0; j < h; j++)
                cout << "*";
				
            cout << endl;
        }
		
        if(n > 0)
            cout << endl;
    }

    return 0;
}
